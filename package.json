    "@vueuse/core": "^10.9.0",
    "flexsearch": "^0.7.43",
    "mark.js": "^8.11.1",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "vitepress": "^1.1.4",
    "vitepress-plugin-search": "^1.0.4-alpha.22"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {
